<?xml version="1.0"?>
<csv-inputs xmlns="http://axelor.com/xml/ns/data-import" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/data-import https://axelor.com/xml/ns/data-import/data-import_6.1.xsd">

  <adapter name="Boolean" type="com.axelor.data.adapter.BooleanAdapter">
    <option name="falsePattern" value="(0|false|no|f|n)"/>
  </adapter>

  <input file="productos.csv" type="com.eyc.base.db.Producto" 
       search="self.descripcion = :descripcion" update="false">
     <bind column="descripcion" to="descripcion" />
     <bind column="trademark" to="marca" />
     <bind column="category" to="categoria" />
     <bind column="precio" to="precio" />
     <bind column="activo" to="activo" adapter="Boolean" />

  </input> 
  
</csv-inputs>
