<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models https://axelor.com/xml/ns/domain-models/domain-models_6.1.xsd">

  <module name="base" package="com.eyc.base.db" />

  <entity name="Category" cacheable="false">
     <one-to-many name="products" ref="Product" 
         mappedBy="category" title="Products" />
       <string name="description"  namecolumn="true" title="Description" />
  </entity>

</domain-models>